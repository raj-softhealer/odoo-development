<odoo>
  <data>

    <!-- form view -->
    <record id="view_stock_wizard" model="ir.ui.view">
      <field name="name">stock.view.wizard.form</field>
      <field name="model">stock.view.wizard</field>
      <field name="arch" type="xml">
        <form>

                <field name="view_stock" readonly="1">
                  <list>
                    <field name="product_id"/>
                    <field name="location_id"/>
                    <field name="inventory_quantity_auto_apply"/>
                    <field name="reserved_quantity"/>
                    <field name="company_id"/>
                  </list>
                </field>
    




          <footer>
            <button string="Go Back" type="object" name="action_go_back" class="oe_highlight"/>
          </footer>
        </form>
      </field>
    </record>


  </data>
</odoo>